#include<stdio.h>
int main()
{
 	int i,j,n,k;
	printf("enter the number");
 	scanf("%d",&n);
 	for(i=1;i<=n;i++)
	{
		printf("n=%d, i=%d, k=%d\n",n,i,n-i);
		k = n;
		for(k=n-i;k<0;k--)
		{
			printf("=");
		}
		//printf("\n");
		for(j=0;j<i;j++)
		{
			printf("* ");
		}		
		printf("\n");
	}
	return 0;
}

